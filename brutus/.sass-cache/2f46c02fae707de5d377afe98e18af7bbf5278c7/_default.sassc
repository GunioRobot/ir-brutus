@font-face
  font-family: 'UniversCondensedBold'
  src: url('univers_condensed_bold-webfont.eot')
  src: url('univers_condensed_bold-webfont.eot?#iefix') format('embedded-opentype'),url('univers_condensed_bold-webfont.woff') format('woff'),url('univers_condensed_bold-webfont.ttf') format('truetype'),url('univers_condensed_bold-webfont.svg#UniversCondensedBold') format('svg')
  font-weight: normal
  font-style: normal

body
  margin: 0
  font: 13px / 1.5em "Helvetica Neue", helvetica, Arial, Verdana, Geneva, sans-serif
  color: #64665c
  background: transparent url(../images/background-02.png) 0 0

body.cke_show_borders
  background-image: none !important

form
  padding: 0 0 0 10px

input.form-text, textarea
  border: 1px solid #b3b3b3
  border-bottom-color: #d9d9d9
  border-left-color: #d9d9d9
  border-radius: 3px
  -khtml-border-radius: 3px
  -moz-border-radius: 3px
  -webkit-border-radius: 3px

input.form-text
  background-color: #f2f2f2
  min-height: 20px
  &:hover, &:focus
    background-color: #fff
    border-color: #666

input.form-submit
  border: 1px solid #cdcdcd
  border-radius: 3px
  -khtml-border-radius: 3px
  -moz-border-radius: 3px
  -webkit-border-radius: 3px
  min-height: 20px
  cursor: pointer

#content input.form-submit
  padding: 5px 10px
  margin: 10px 0
  background: transparent url(../images/back-submit-btn.png) 0 0 repeat-x
  color: #666
  font-weight: bold

#content input#edit-delete
  background-color: red

#edit-actions
  border-top: 1px solid #dcdcdc

.block
  border-radius: 3px
  -khtml-border-radius: 3px
  -moz-border-radius: 3px
  -webkit-border-radius: 3px

// Links

a
  &:link, &:visited
    color: #5d2067
    text-decoration: none
    border-bottom: 0
  &:hover, &:active
    text-decoration: underline
    border-bottom: 0

.views-field-view-node
  text-align: right
  span.field-content
    a
      font-size: .85em
      display: inline-block
      height: 13px
      background: transparent url(../images/icons/ico-read-more.png) 0 0 no-repeat
      padding-left: 17px
      line-height: 14px

#main
  margin-bottom: 20px !important
  a
    &:link, &:visited
      color: #5d2067
      text-decoration: none
    &:hover, &:active
      text-decoration: underline
  #content
    margin: 0 10px 0 0 !important
  #sidebar-first
    margin: 0 10px
    float: left
    ul li
      border-bottom: 1px solid #dcdcdc
      margin-bottom: 5px
  #sidebar-second
    margin: 0 0 0 10px
    float: right

.page-admin, .no-sidebars
  #main
    #content
      width: 100% !important
      margin: 0 !important

ul.primary li a, .menu-name-primary-links li a
  border: 0

.two-sidebars .center, .sidebar-right .center
  margin: 0 !important

.two-sidebars .center, .sidebar-left .center
  margin: 0 !important

#sidebar-first
  padding-top: 20px
  ul, ol, ul li, ol li
    margin: 0
    padding: 0
    list-style-type: none
  .block
    background-color: #fff

// Titles.

h1, h2, h3, h4, h5, h6
  line-height: .95em
  font-family: 'UniversCondensedBold'
  color: #333
  font-weight: normal !important

h1
  font-size: 2.2em
  font-weight: 300
  padding: 0 0 0.5em
  margin: 0

h2
  font-size: 1.8em
  font-weight: 300
  margin-top: 0
  margin-bottom: 10px

h3
  font-size: 1.4em
  margin-bottom: 10px

h3.title
  border-bottom: 1px solid #dcdcdc
  font-size: 1em
  font-weight: 400
  text-transform: uppercase

h4
  font-size: 1.2em
  margin-bottom: 10px

h5
  font-size: 1.1em
  font-weight: 600
  margin-bottom: 0

h6
  font-size: 1em
  font-weight: bold

// Paragraph styles.
p
  margin: 0 0 1em 0

// List styles.

ul, ol
  margin-left: 0
  padding-left: 2em
  margin-bottom: 15px

.node-type-noticias
  .node-inner
    .content
      ul
        li
          ul
            li
              margin-top: 0

// Table styles.

table
  width: 99%

tr
  &.even td
    padding: 10px 5px
  &.odd
    td
      padding: 10px 5px
    background-color: white

// Code.

pre, code, tt
  font: 1em "andale mono", "lucida console", monospace
  line-height: 1.5

pre
  background-color: #efefef
  display: block
  padding: 5px
  margin: 5px 0
  border: 1px solid #aaaaaa

// List styles.

ul
  list-style-type: disc
  ul
    list-style-type: circle
    ul
      list-style-type: square
      ul
        list-style-type: circle

ol
  list-style-type: decimal
  ol
    list-style-type: lower-alpha
    ol
      list-style-type: decimal

// Abbreviation.

abbr
  border-bottom: 1px dotted #666666
  cursor: help
  white-space: nowrap

// UI improvments.

#edit-title
  font-size: 24px
  width: 98%

#system-themes-form img
  width: 100px

.form-item .description
